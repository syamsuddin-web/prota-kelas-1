    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Mengatur navigasi antar mata pelajaran
            const subjectButtons = document.querySelectorAll('.subject-btn');
            const subjectContents = document.querySelectorAll('.subject-content');
            
            subjectButtons.forEach(button => {
                button.addEventListener('click', function() {
                    const subjectId = this.getAttribute('data-subject');
                    
                    // Menghapus kelas aktif dari semua tombol dan konten
                    subjectButtons.forEach(btn => btn.classList.remove('active'));
                    subjectContents.forEach(content => content.classList.remove('active'));
                    
                    // Menambahkan kelas aktif ke tombol yang diklik
                    this.classList.add('active');
                    
                    // Menampilkan konten yang sesuai
                    document.getElementById(subjectId).classList.add('active');
                });
            });
            
            // Menambahkan fitur untuk menyimpan sebagai file HTML offline
            const saveBtn = document.createElement('a');
            saveBtn.className = 'download-btn';
            saveBtn.innerHTML = '<i class="fas fa-download"></i> Simpan sebagai HTML Offline';
            saveBtn.href = 'javascript:void(0)';
            saveBtn.onclick = function() {
                const htmlContent = document.documentElement.outerHTML;
                const blob = new Blob([htmlContent], {type: 'text/html'});
                const url = URL.createObjectURL(blob);
                const a = document.createElement('a');
                a.href = url;
                a.download = 'PROTA_Kelas_1_UPT_SD_Negeri_55_Otting.html';
                document.body.appendChild(a);
                a.click();
                document.body.removeChild(a);
                URL.revokeObjectURL(url);
            };
            
            document.querySelector('.navigation').appendChild(saveBtn);
        });
    </script>

</body></html>
